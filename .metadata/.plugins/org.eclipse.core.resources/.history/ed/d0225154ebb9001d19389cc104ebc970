
public class Carro {
	
	// Atributos
	
	String cor;
	String modelo;
	int capacidadeDoTanque;
	
	// Metodos
	
	public boolean ligar() {
		return true;
	}
	
	public boolean desligar() {
		return false;
	}

	// Construtor
	
	public Carro() {
		cor = "verde";
	}
	
	// Destrutor - Só é necessario se for preciso liberar memoria
	
	protected void finalize() {
		
	}

}


